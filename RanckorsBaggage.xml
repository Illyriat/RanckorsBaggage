<GuiXml>
    <!-- Main Window -->
    <TopLevelControl name="RanckorsBaggageWindow" mouseEnabled="true" movable="true" clampedToScreen="true">
        <Dimensions x="300" y="300"/>
        <Controls>
            <!-- Background -->
            <Backdrop name="RanckorsBaggageWindowBG" inherits="ZO_DefaultBackdrop" hidden="false">
                <Anchor point="TOPLEFT"/>
                <Anchor point="BOTTOMRIGHT"/>
                <CenterColor r="0" g="0" b="0" a="0.5"/>
                <EdgeColor r="0" g="0" b="0" a="1"/>
                <EdgeFile file="" edgeSize="8"/>
            </Backdrop>
            <!-- Label for displaying information -->
            <Label name="RanckorsBaggageWindowLabel" font="ZoFontGameLarge" color="1,1,1,1" hidden="false">
                <Dimensions x="300" y="300
                "/>
                <Anchor point="CENTER" relativeTo="RanckorsBaggageWindow" relativePoint="CENTER"/>
                <HorizontalAlignment>LEFT</HorizontalAlignment>
                <VerticalAlignment>TOP</VerticalAlignment>
                <Text/>
            </Label>
        </Controls>
        <OnMoveStop>
            RanckorsBaggage:OnIndicatorMoveStop()
        </OnMoveStop>
    </TopLevelControl>
</GuiXml>
